<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>


<AnchorPane fx:id="titleBar" maxWidth="1.7976931348623157E308" minHeight="35.0" prefHeight="35.0" prefWidth="200.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mid.ui.scene.StyleStageController">
    <children>
        <HBox alignment="CENTER" layoutX="5.0" layoutY="5.0" spacing="10.0" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="5.0" AnchorPane.topAnchor="5.0">
            <children>
                <ImageView fx:id="stageIcon" fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true" />
                <Label fx:id="title" text="title" textFill="#d6d6d6" />
            </children>
        </HBox>
        <HBox layoutX="560.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <JFXButton contentDisplay="GRAPHIC_ONLY" maxHeight="25.0" minHeight="25.0" onAction="#minimize" styleClass="toolbar-button" textFill="WHITE">
                    <graphic>
                        <FontAwesomeIconView fill="#d6d6d6" glyphName="WINDOW_MINIMIZE" size="12" />
                    </graphic>
                </JFXButton>
                <JFXButton contentDisplay="GRAPHIC_ONLY" layoutX="10.0" layoutY="10.0" maxHeight="25.0" minHeight="25.0" onAction="#close" styleClass="toolbar-button">
                    <graphic>
                        <FontAwesomeIconView fill="#d6d6d6" glyphName="REMOVE" size="14" />
                    </graphic>
                </JFXButton>
            </children>
        </HBox>
    </children>
</AnchorPane>
